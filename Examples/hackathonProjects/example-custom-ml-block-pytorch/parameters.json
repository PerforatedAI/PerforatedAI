{
    "version": 1,
    "type": "machine-learning",
    "info": {
        "name": "Dendritic NN Block (PyTorch)",
        "description": "An ML block that implements a PyTorch neural network with Dendritic Optimization.  Dendritic Optimization is a technique that enhances the learning capacity of neural networks by mimicking the dendritic structures found in biological neurons. This block allows users to create custom neural network architectures with configurable layers and apply Dendritic Optimization to improve performance on various tasks.",
        "operatesOn": "other",
        "indRequiresGpu": false,
        "repositoryUrl": "https://github.com/edgeimpulse/example-custom-ml-block-pytorch"
    },
    "parameters": [
        {
            "name": "Batch Size",
            "value": "128",
            "type": "int",
            "param": "batch-size"
        },
        {
            "name": "Learning rate",
            "value": "0.005",
            "type": "float",
            "help": "How fast the neural network learns, if the network overfits quickly, then lower the learning rate.",
            "param": "learning-rate"
        },
        {
            "name": "Seed",
            "value": "0",
            "type": "int",
            "help": "Controls the random seed.",
            "param": "seed"
        },
        {
            "name": "Noise Standard Deviation",
            "value": "None",
            "type": "select",
            "param": "noise-std",
            "valid": [
                "None",
                "Low",
                "High"
            ],
            "help": "Gaussian noise augmentation during training. Low=0.2, High=0.45."
        },
        {
            "name": "Mask Time Bands",
            "value": "None",
            "type": "select",
            "param": "mask-time-bands",
            "valid": [
                "None",
                "Low",
                "High"
            ],
            "help": "Time band masking (SpecAugment). Low=1 band/1 consecutive, High=3 bands/2 consecutive."
        },
        {
            "name": "Mask Frequency Bands",
            "value": "None",
            "type": "select",
            "param": "mask-freq-bands",
            "valid": [
                "None",
                "Low",
                "High"
            ],
            "help": "Frequency band masking (SpecAugment). Low=1 band/4 consecutive, High=3 bands/4 consecutive."
        },
        {
            "name": "Warp Time",
            "value": "false",
            "type": "boolean",
            "param": "warp-time",
            "help": "Enable time warping augmentation for spectrograms."
        },
        {
            "name": "Auto Weight Classes",
            "value": "false",
            "type": "boolean",
            "param": "auto-weight-classes",
            "help": "Automatically weight classes based on sample counts to handle class imbalance."
        },
        {
            "name": "Dendritic Optimization",
            "value": "true",
            "type": "boolean",
            "help": "Controls whether dendritic optimization should be applied.",
            "param": "dendritic-optimization"
        },
        {
            "name": "Speed Switch",
            "value": "slow",
            "type": "select",
            "help": "Controls the speed to switch.",
            "valid": [
                "extra-slow",
                "slow",
                "medium",
                "fast"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "switch-speed"
        },
        {
            "name": "Maximum number of Dendrites",
            "value": "1",
            "type": "int",
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "max-dendrites"
        },
        {
            "name": "Improvement Threshold",
            "value": "medium",
            "type": "select",
            "valid": [
                "low",
                "medium",
                "high"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "improvement-threshold"
        },
        {
            "name": "Dendrite Weight Initialization Multiplier",
            "value": "0.01",
            "type": "float",
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-weight-initialization-multiplier"
        },
        {
            "name": "Dendrite Forward Function",
            "value": "relu",
            "type": "select",
            "valid": [
                "relu",
                "sigmoid",
                "tanh"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-forward-function"
        },
        {
            "name": "Dendrite Conversion",
            "value": "Linear Only",
            "type": "select",
            "valid": [
                "Linear Only",
                "All Layers"
            ],
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "dendrite-conversion"
        },
        {
            "name": "Improved Dendritic Optimization",
            "value": "false",
            "type": "boolean",
            "help": "Controls whether improved dendritic optimization should be applied.",
            "showIf": {
                "parameter": "dendritic-optimization",
                "operator": "eq",
                "value": "true"
            },
            "param": "improved-dendritic-optimization"
